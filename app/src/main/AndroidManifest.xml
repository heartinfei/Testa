<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wq.labelviewsample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewGroupSampleActivity"
            android:label="@string/title_activity_view_group_sample" >
        </activity>
        <activity
            android:name=".RecyclerviewSampleActivity"
            android:label="@string/title_activity_recyclerview_sample" >
        </activity>
    </application>

</manifest>
